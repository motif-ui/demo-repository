  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.es.js",
      "require": "./dist/hooks.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.es.js",
      "require": "./dist/types.cjs"
    },
    "./themes/*": "./dist/themes/*",
    "./*": "./dist/*"
  }
