  "private": false,
  "scripts": {
    "build": "next build",
    "dev": "next dev --webpack",
    "start": "next start",
    "test": "jest --watch",
    "test:update": "npm run test:ci -- -u",
    "test:ci": "jest --ci",
    "build:lib": "rollup -c",
    "build:storybook": "storybook build --stats-json",
    "build:tokens": "node src/scripts/buildToken",
    "build:theme": "node src/scripts/buildTheme",
    "build:theme-default": "npm run build:theme src/lib/styles/tokens/design-tokens.tokens.json default-theme src/lib/styles/themes",
    "lint": "eslint ./src",
    "lint:staged": "eslint --cache",
    "prepare": "husky",
    "semantic-release": "semantic-release",
    "storybook": "storybook dev -p 6006",
    "chromatic": "npx chromatic --project-token=chpt_23a36ef493129f4 --build-script-name build:storybook",
    "chromatic:cli": "npm run build:storybook && npx chromatic --exit-zero-on-changes -d storybook-static/",
    "type-check": "tsc --noemit",
    "maintanence-check": "npm run test:ci && npm run build:lib && npm run build:theme-default && npm run build && npm run dev"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
